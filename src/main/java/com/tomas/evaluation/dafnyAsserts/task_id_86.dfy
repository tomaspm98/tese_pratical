method CenteredHexagonalNumberTest(){
  var res1:= CenteredHexagonalNumber(10);
  print(res1); print("\n");
  assert res1==271;

  var res2:= CenteredHexagonalNumber(2);
  print(res2); print("\n");
  assert res2==7;

  var res3:= CenteredHexagonalNumber(9);
  print(res3); print("\n");
  assert res3==217;

}